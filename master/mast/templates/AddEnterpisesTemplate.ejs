<div class="add-enterprises-template" style="background-clip: padding-box;border-radius: 7px;box-shadow: 0 1px 5px #b3bac4;margin: 40px 2%;padding: 10px;">

	<!-- upgrade header -->
	<div class="title-mod">
		<div class="title-fix">
 			<span> Add New Enterprise </span>
 			<a class="add-subscription">
 				<input type="button" value="Back" class="btn btn-primary" onclick=" window.history.back()">
			</a>
			<div class="clear"></div>
		</div>
	</div>

<!-- settings content -->
	<div class="main-mod ent-form-cont" id="main-mod"> 
		<!-- company details section -->
		<form action="" method="POST" class="new-user-form">
			<div class="center-mod">
				<!-- <h3 class="company-details">New Enterprise</h3> -->
				<div class="company-name">Enterprise Name</div>
					<input type="text" name="enterprises_name" placeholder="Enter First Name" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;">

				<div class="company-name">Owner Name</div>
				<input type="text" name="owner_name" placeholder="Enter Owner Name" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;">

				<div class="permission-form">
				<div class="company-name">Email</div>
					<input type="text" name="email" placeholder="Enter Email" class="enter_name accounts" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;">
				</div>

				<div class="company-name"> Password </div>
				<input type="password" name="password" placeholder="Enter Password" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;">

				<div class="company-name"> Confirm Password </div>
				<input type="password" name="c_password" placeholder="Confirm Password" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;">


				<!-- <div class="company-name"> Title </div>
				<input type="text" name="title" placeholder="Enter Title" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;"> -->

				<!-- <div class="company-name"> Maximum Number Of Users </div>
				<input type="text" name="user" placeholder="Maximum Number Of Users" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;">

				<div class="company-name"> Maximum Number Of Workgroups </div>
				<input type="text" name="workgroup" placeholder="Maximum Number Of Workgroups" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;"> -->

				<!-- <div class="company-name"> Quota (GB) </div>
				<input type="text" name="quota" placeholder="Enter Quota in GB" class="enter_name" style="border: 1px solid @borderGray;
				margin-bottom:12px;width: 275px;padding: 7px 14px;">
				<div class="clear"></div> -->

				<div class="company-name"> Subscription</div>
				<select id="subscription-drop" name="subscription" style="border: 1px solid #d7dbdc;margin-bottom: 12px;padding: 10px 15px;width: 305px;"></select>
				<div class="clear"></div>

			</div>  
		</form>
		<div class="clear"></div> 
	</div> 

	<!-- save button -->
	<div class="footer-mod ent-form-cont">
		<a class="setting-save-button add-enterprises">
			<img src="/images/button_save@2x.png" />
		</a>
	</div>
	<div class="footer-mod add-subs-cont">
		<div class="company-name"> No subscription added yet. Please add
			subscription plan.
			<a href="#addsubscription" class="add-subscription">
			Add Subscription
		</a>
		</div>
	</div>
</div>










